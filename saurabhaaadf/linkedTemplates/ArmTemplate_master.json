{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "saurabhaaadf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabaseLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLS'"
		},
		"AzureSqlDatabaseSampledbLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseSampledbLS'"
		},
		"AzureSqlDatabasetrialLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabasetrialLS'"
		},
		"SqlServerLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerLS'"
		},
		"SqlServerLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerLS'"
		},
		"SqlServernewLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServernewLS'"
		},
		"SqlServernewLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServernewLS'"
		},
		"SqlServerLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Saurabh"
		},
		"SqlServernewLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Saurabh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "saurabhaaadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabaseLS_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLS_connectionString')]"
					},
					"AzureSqlDatabaseSampledbLS_connectionString": {
						"value": "[parameters('AzureSqlDatabaseSampledbLS_connectionString')]"
					},
					"AzureSqlDatabasetrialLS_connectionString": {
						"value": "[parameters('AzureSqlDatabasetrialLS_connectionString')]"
					},
					"SqlServerLS_connectionString": {
						"value": "[parameters('SqlServerLS_connectionString')]"
					},
					"SqlServerLS_password": {
						"value": "[parameters('SqlServerLS_password')]"
					},
					"SqlServernewLS_connectionString": {
						"value": "[parameters('SqlServernewLS_connectionString')]"
					},
					"SqlServernewLS_password": {
						"value": "[parameters('SqlServernewLS_password')]"
					},
					"SqlServerLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerLS_properties_typeProperties_userName')]"
					},
					"SqlServernewLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServernewLS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}